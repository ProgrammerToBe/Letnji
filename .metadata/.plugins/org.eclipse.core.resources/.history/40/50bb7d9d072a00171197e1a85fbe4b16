administrator.controller('Opsti', function($scope, $http, $compile, $timeout){
	
	$scope.tab = 0;
	
	$scope.init = function(){
		
		
		
	};
	

	this.tabClick = function(num){
		
		$scope.tab = num;
		
	};
	
	this.tabClick2 = function(num, drzave){
		
		$scope.tab = num;
		
		$scope.dr = drzave;
	};
	
});