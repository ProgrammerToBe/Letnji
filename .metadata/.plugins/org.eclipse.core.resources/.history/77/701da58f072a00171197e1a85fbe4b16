administrator.controller('Opsti', function($scope, $http, $compile, $timeout){
	
	$scope.tab = 0;
	
	$scope.init = function(){
		
		
		
	};
	

	this.tabClick = function(num){
		
		$scope.tab = num;
		
	};
	
	this.tabClick2 = function(num){
		
		$scope.tab = num;
		
		$timeout(function() {
		    angular.element('#nm').triggerHandler('click');
		  });
	};
	
});